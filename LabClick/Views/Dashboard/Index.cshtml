
@{
                /**/

                ViewBag.Title = "Dashboard";
                Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="row">
    
    <div class="col-lg-6">
        <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="~/Content/styles/images/ico-pessoas.png" />

            <div class="card-body">
                <h5 class="card-title">Pacientes, Testes e Laudos</h5>
                <p class="card-text"></p>
                <a href="@Url.Action("Index", "Paciente")" class="btn btn-primary">Ver Pacientes</a>
            </div>
        </div>
    </div>
        
    <div class="col-lg-6">
        <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="~/Content/styles/images/novopaciente.png" />

            <div class="card-body">
                <h5 class="card-title">Cadastrar novo paciente</h5>
                <p class="card-text"></p>
                <a href="@Url.Action("NovoPaciente", "Paciente")" class="btn btn-primary">Novo Paciente</a>
            </div>
        </div>
    </div>
        
</div>

